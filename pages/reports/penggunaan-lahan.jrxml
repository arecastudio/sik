	<queryString>
		<![CDATA[SELECT t.id,t.nama AS nama2,j.id,j.nama,t.satuan,IFNULL(td.luas,0),(SELECT MAX(id) FROM db_sik.tanah_guna),(SELECT CONCAT(desa_kelurahan,', ',kabupaten_kota,', ',provinsi) AS kampung FROM db_sik.pokok_desa WHERE id=1) AS nkampung FROM db_sik.tanah_guna AS t INNER JOIN db_sik.jenis_tanah AS j ON j.id=t.id_jenis LEFT OUTER JOIN db_sik.tanah_desa AS td ON td.id_tanah_guna=t.id AND td.id_pokok_desa=1 WHERE 1 ORDER BY j.nama ASC;]]>
